<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.donzzul.spring.reservation.domain.Reservation" alias="Reservation"/>
		<typeAlias type="com.donzzul.spring.dreamreview.domain.DreamReview" alias="DreamReview"/>
		<typeAlias type="com.donzzul.spring.mzreview.domain.MzReview" alias="MzReview"/>
		<typeAlias type="com.donzzul.spring.mzreview.domain.MzReviewPhoto" alias="MzReviewPhoto"/>
		<typeAlias type="com.donzzul.spring.notiqna.domain.Notice" alias="Notice"/>
		<typeAlias type="com.donzzul.spring.notiqna.domain.Qna" alias="Qna"/>
		<typeAlias type="com.donzzul.spring.recommendboard.domain.RecommendBoard" alias="RecommendBoard"/>
		<typeAlias type="com.donzzul.spring.recommendboard.domain.RecommendPhoto" alias="RecommendPhoto"/>
		<typeAlias type="com.donzzul.spring.payment.domain.Don" alias="Don"/>
		<typeAlias type="com.donzzul.spring.user.domain.User" alias="User"/>
		<typeAlias type="com.donzzul.spring.shop.domain.Shop" alias="Shop"/>
	    <typeAlias type="com.donzzul.spring.shop.domain.MainMenu" alias="MainMenu"/>
	    <typeAlias type="com.donzzul.spring.shop.domain.MenuPhoto" alias="MenuPhoto"/>
	    <typeAlias type="com.donzzul.spring.pick.domain.Pick" alias="Pick"/>
	    <typeAlias type="com.donzzul.spring.chatting.domain.Chat" alias="Chat"/>
	</typeAliases>
	
	

	<mappers>
		<mapper resource="mappers/reservation-mapper.xml"></mapper>
		<mapper resource="mappers/mzReview-mapper.xml"></mapper>
		<mapper resource="mappers/drmReview-mapper.xml"></mapper>
		<mapper resource="mappers/recommend-mapper.xml"></mapper>
		<mapper resource="mappers/notiqna-mapper.xml"></mapper>
		<mapper resource="mappers/user-mapper.xml"></mapper>
		<mapper resource="mappers/shop-mapper.xml"></mapper>
		<mapper resource="mappers/pick-mapper.xml"></mapper>
		<mapper resource="mappers/payment-mapper.xml"></mapper>
		<mapper resource="mappers/chat-mapper.xml"></mapper>
	</mappers>
</configuration>